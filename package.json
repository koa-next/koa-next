{
  "name": "koa-next",
  "version": "1.0.0",
  "description": "A FrameWork Based On Koa.js And Next.js",
  "main": "index.js",
  "scripts": {
    "test": "jest --forceExit",
    "tslint": "tslint -c tslint.json -p tsconfig.koa.json && tslint -c tslint.json -p tsconfig.json",
    "tsc": "tsc -p tsconfig.koa.json",
    "build": "npm run tsc && npm run next-build",
    "next-build": "next build client",
    "next-dev": "next dev client",
    "dev": "nodemon --config nodemon.json",
    "start": "cross-env NODE_ENV=prod node server.js",
    "clean": "ts-clean"
  },
  "author": " <PLDaily pcd12321@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.37",
    "@types/lodash": "^4.14.119",
    "@types/next": "^7.0.6",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "ts-clean": "1.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-react": "^3.6.0"
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "antd": "^3.13.2",
    "babel-plugin-import": "^1.11.0",
    "css-loader": "1.0.0",
    "extracted-loader": "^1.0.7",
    "koa": "^2.6.2",
    "koa-log4": "^2.3.2",
    "koa-next-middleware": "^1.0.0",
    "koa-router": "^7.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "next": "^7.0.2",
    "next-redux-wrapper": "2.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0",
    "supertest": "^3.4.2",
    "typescript": "^3.2.2",
    "webpack": "^4.29.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint && git add ."
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koa-next/koa-next.git"
  },
  "keywords": [
    "koa.js",
    "next.js",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/koa-next/koa-next/issues"
  },
  "homepage": "https://github.com/koa-next/koa-next#readme"
}
