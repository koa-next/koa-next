{
  "presets": [
    ["next/babel", {
      "preset-env": {
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie > 9"
          ]
        },
        "useBuiltIns": "usage",
        "loose": true
      }
    }],
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
  ],
}
