{
  "name": "koa-next",
  "version": "2.0.0",
  "description": "A FrameWork Based On Koa.js And Next.js",
  "main": "index.js",
  "scripts": {
    "test": "NEXT_ENV=test jest --forceExit",
    "tslint": "tslint -c tslint.json -p tsconfig.koa.json && tslint -c tslint.json -p client/tsconfig.json",
    "tsc": "tsc -p tsconfig.koa.json",
    "build": "npm run tsc && npm run next-build",
    "next-build": "next build client",
    "next-dev": "next dev client",
    "dev": "nodemon --config nodemon.json",
    "start": "node server.js",
    "docker": "sh .docker/build.sh && sh .docker/start.sh",
    "release": "standard-version",
    "clean": "ts-clean"
  },
  "author": " <PLDaily pcd12321@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
