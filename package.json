    "@types/node": "^10.12.18",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/supertest": "^2.0.7",
